<grammar>
	<root-symbol>polinomio</root-symbol>
	<terminal-symbols>
		<symbol>
			<name>+</name>
			<code>symreg.Add</code>
		</symbol>		
		<symbol>		
			<name>-</name>
			<code>symreg.Sub</code>
		</symbol>
		<symbol>
			<name>*</name>
			<code>symreg.Mul</code>
		</symbol>	
		<symbol>
			<name>sin</name>
			<code>symreg.Sin</code>
		</symbol>		
		<symbol>
			<name>cos</name>
			<code>symreg.Cos</code>
		</symbol>		
		<symbol>
			<name>x</name>
			<code>symreg.X</code>
		</symbol>
		<symbol>
			<name>cte</name>
			<code>symreg.Const</code>
		</symbol>
	</terminal-symbols>
	<non-terminal-symbols>
		<symbol>
			<name>polinomio</name>
			<production-rule>
				<element>+</element>
				<element>monomio</element>
				<element>polinomio</element>
			</production-rule>
		</symbol>
		<symbol>
			<name>polinomio</name>
			<production-rule>
				<element>-</element>
				<element>monomio</element>
				<element>polinomio</element>
			</production-rule>
		</symbol>
		<symbol>
			<name>polinomio</name>
			<production-rule>
				<element>monomio</element>
			</production-rule>
		</symbol>
		<symbol>
			<name>monomio</name>
			<production-rule>
				<element>sin</element>
				<element>polinomio</element>
			</production-rule>
		</symbol>
		<symbol>
			<name>monomio</name>
			<production-rule>
				<element>cos</element>
				<element>polinomio</element>
			</production-rule>
		</symbol>		
		<symbol>
			<name>monomio</name>
			<production-rule>
				<element>x</element>
			</production-rule>
		</symbol>
		<symbol>
			<name>monomio</name>
			<production-rule>
				<element>cte</element>
			</production-rule>
		</symbol>		
		<symbol>
			<name>monomio</name>
			<production-rule>
				<element>*</element>
				<element>x</element>
				<element>monomio</element>
			</production-rule>
		</symbol>
		<symbol>
			<name>monomio</name>
			<production-rule>
				<element>*</element>
				<element>cte</element>
				<element>monomio</element>
			</production-rule>
		</symbol>
	</non-terminal-symbols>
</grammar>
